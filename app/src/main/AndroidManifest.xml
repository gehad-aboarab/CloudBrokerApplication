<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cmp404.cloud_brokerapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".Android.BrokerApplication"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".Android.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Android.LoginActivity" />
        <activity android:name=".Android.SignupActivity" />
        <activity android:name=".Android.ForgotPasswordActivity" />
        <activity android:name=".Android.DashboardActivity" />
        <activity android:name=".Android.ProfileActivity" />
        <activity android:name=".Android.InsuranceActivity" />
        <activity android:name=".Android.TestingCenterActivity" />
        <activity android:name=".Android.DubaiPoliceActivity" />
        <activity android:name=".Android.RtaActivity" />
        <activity android:name=".Android.EditProfileActivity"></activity>
    </application>

</manifest>